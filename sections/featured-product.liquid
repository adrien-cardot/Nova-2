											id="{{ block.id }}"
											{{ block.shopify_attributes }}
										>
											{% if section.settings.product.selected_or_first_available_variant.sku != blank %}
												<p>
													{{ 'products.product.sku' | t }}:
													{{ section.settings.product.selected_or_first_available_variant.sku }}
												</p>
											{% endif %}
										</div>
									{% when 'complementary_products' %}
										<div class="product-info__block" {{ block.shopify_attributes }}>
											{% if section.settings.product.metafields['shopify--discovery--product_recommendation'].complementary_products.value.count
													> 1
											-%}
												{%- if block.settings.layout == 'list' -%}
													{%- if block.settings.heading != blank %}
														<h2 class="product-info__heading product-info__complementary__heading">
															{{ block.settings.heading }}
														</h2>
													{% endif %}
													<div style="--stack-spacing:0.5" class="stack stack--v">
														{% for c_product in section.settings.product.metafields['shopify--discovery--product_recommendation'].complementary_products.value -%}
															{%- assign src_image = c_product.featured_image %}

															<div class="product-thumb product-thumb--border">
																{% capture capture_card %}
																	<picture class="product-thumb__image">
																		<source srcset="{{ src_image | image_url: width: 70 }} 1x, {{ src_image | image_url: width: 140 }} 2x, {{ src_image | image_url: width: 210 }} 3x">

																		<img
																			src="{{ src_image | image_url }}"
																			alt="{{ src_image.alt | escape }}"
																			width="{{ src_image.width }}"
																			height="{{ src_image.height }}"
																			loading="lazy"
																			data-img-loading
																			onload="this.removeAttribute(&#34;data-img-loading&#34;)"
																		>
																	</picture>
																	<div class="product-thumb__info">
																		<p
																			class="product-thumb__title {% if settings.product_card_truncate_titles != 'none' %}product-thumb__title--truncate{% endif %}"
																			style="--product-thumb-max-title-lines: {{ settings.product_card_truncate_titles }};"
																		>
																			{{ c_product.title }}
																		</p>

																		<div style="--stack-spacing:0.25" class="stack product-thumb__price">
																			<span class="product-thumb__price-value">
																				{% if settings.currency_hide_trailing_zeros -%}
																					{{- c_product.price | money_without_trailing_zeros -}}
																				{%- else -%}
																					{{- c_product.price | money -}}
																				{%- endif -%}
																				{%- if settings.currency_show_currency_codes -%}
																					{{- shop.currency | prepend: '&nbsp;' -}}
																				{%- endif %}
																			</span>
																		</div>
																	</div>
																{% endcapture -%}
																{%- if block.settings.enable_quick_view -%}
																	<x-modal
																		class="modal"
																		id="{{ c_product.handle | prepend: 'quick-view-modal' }}"
																		url="{{ c_product.url | split: '?' | first }}"
																		url-section="product-quick-view"
																		style=""
																	>
																		<dialog class="modal__dialog" data-modal-element="dialog">
																			<div class="modal__content" data-modal-element="content"></div>
																		</dialog>
																	</x-modal>

																	<x-modal-trigger
																		class="modal-trigger product-thumb__body"
																		target-id="{{ c_product.handle | prepend: 'quick-view-modal' }}"
																		tabindex="0"
																		role="button"
																	>
																		{{ capture_card }}
																	</x-modal-trigger>
																{%- else -%}
																	<a class="block-link product-thumb__body" href="{{ c_product.url }}">
																		{{ capture_card }}
																	</a>
																{%- endif %}
															</div>
														{% endfor %}
													</div>
												{% else -%}
													{{- 'component-inline-carousel.css' | asset_url | stylesheet_tag }}
													<x-inline-carousel
														class="inline-carousel inline-carousel--compact"
														items-count="{{ section.settings.product.metafields.shopify--discovery--product_recommendation.complementary_products.value.count }}"
														columns="1"
														style="--inline-carousel-columns: 1;--inline-carousel-mobile-columns: 1.25;--inline-carousel-items-count: {{ section.settings.product.metafields.shopify--discovery--product_recommendation.complementary_products.value.count }};"
														section-id="{{ section.id }}"
													>
														<div class="inline-carousel__header">
															{% if block.settings.heading != blank %}
																<h2 class="product-info__heading product-info__complementary__heading">
																	{{ block.settings.heading }}
																</h2>
															{% endif %}
															<div class="inline-carousel__header-nav">
																<button
																	class="inline-carousel__nav-prev"
																	aria-label="{{ 'general.slider.previous_slide' | t }}"
																	data-inline-carousel-element="nav-prev"
																>
																	<span class="carousel-arrow carousel-arrow--size--x-small carousel-arrow--type--outline">
																		<svg class="carousel-arrow__icon">
																			<use  xlink:href="#svg-icon-arrow-left"></use>
																		</svg>
																	</span>
																</button>
																<button
																	class="inline-carousel__nav-next"
																	aria-label="{{ 'general.slider.next_slide' | t }}"
																	data-inline-carousel-element="nav-next"
																>
																	<span class="carousel-arrow carousel-arrow--size--x-small carousel-arrow--type--outline">
																		<svg class="carousel-arrow__icon">
																			<use  xlink:href="#svg-icon-arrow-right"></use>
																		</svg>
																	</span>
																</button>
															</div>
														</div>
														<div class="inline-carousel__view" data-inline-carousel-element="view">
															<div class="inline-carousel__track" data-inline-carousel-element="track">
																{% for c_product in section.settings.product.metafields['shopify--discovery--product_recommendation'].complementary_products.value -%}
																	{%- assign src_image = c_product.featured_image %}

																	<div class="product-thumb product-thumb--border">
																		{% capture capture_card %}
																			<picture class="product-thumb__image">
																				<source srcset="{{ src_image | image_url: width: 70 }} 1x, {{ src_image | image_url: width: 140 }} 2x, {{ src_image | image_url: width: 210 }} 3x">

																				<img
																					src="{{ src_image | image_url }}"
																					alt="{{ src_image.alt | escape }}"
																					width="{{ src_image.width }}"
																					height="{{ src_image.height }}"
																					loading="lazy"
																					data-img-loading
																					onload="this.removeAttribute(&#34;data-img-loading&#34;)"
																				>
																			</picture>
																			<div class="product-thumb__info">
																				<p
																					class="product-thumb__title {% if settings.product_card_truncate_titles != 'none' %}product-thumb__title--truncate{% endif %}"
																					style="--product-thumb-max-title-lines: {{ settings.product_card_truncate_titles }};"
																				>
																					{{ c_product.title }}
																				</p>

																				<div style="--stack-spacing:0.25" class="stack product-thumb__price">
																					<span class="product-thumb__price-value">
																						{% if settings.currency_hide_trailing_zeros -%}
																							{{- c_product.price | money_without_trailing_zeros -}}
																						{%- else -%}
																							{{- c_product.price | money -}}
																						{%- endif -%}
																						{%- if settings.currency_show_currency_codes -%}
																							{{- shop.currency | prepend: '&nbsp;' -}}
																						{%- endif %}
																					</span>
																				</div>
																			</div>
																		{% endcapture -%}
																		{%- if block.settings.enable_quick_view -%}
																			<x-modal
																				class="modal"
																				id="{{ c_product.handle | prepend: 'quick-view-modal' }}"
																				url="{{ c_product.url | split: '?' | first }}"
																				url-section="product-quick-view"
																				style=""
																			>
																				<dialog class="modal__dialog" data-modal-element="dialog">
																					<div class="modal__content" data-modal-element="content"></div>
																				</dialog>
																			</x-modal>

																			<x-modal-trigger
																				class="modal-trigger product-thumb__body"
																				target-id="{{ c_product.handle | prepend: 'quick-view-modal' }}"
																				tabindex="0"
																				role="button"
																			>
																				{{ capture_card }}
																			</x-modal-trigger>
																		{%- else -%}
																			<a class="block-link product-thumb__body" href="{{ c_product.url }}">
																				{{ capture_card }}
																			</a>
																		{%- endif %}
																	</div>
																{% endfor %}
															</div>
														</div>
														<div class="container">
															<x-carousel-pagination
																class="carousel-pagination inline-carousel__mobile-pagination"
																section-id="{{ section.id }}"
															>
																{% for i in (0..section.settings.product.metafields['shopify--discovery--product_recommendation'].complementary_products.value.count) -%}
																	{%- unless forloop.last -%}
																		<label>
																			<input
																				class="carousel-pagination__radio"
																				type="radio"
																				name="{{ section.id | append: 'carousel-pagination' }}"
																				value="{{ i }}"
																				data-carousel-pagination-element="radio"
																				{% if forloop.first %}
																					checked
																				{% endif %}
																			>
																			<div class="carousel-pagination__dot"></div>
																		</label>
																	{%- endunless -%}
																{%- endfor %}
															</x-carousel-pagination>
															<script type="module" src="{{ 'component-carousel-pagination.js' | asset_url }}"></script>
														</div>
													</x-inline-carousel>

													<script type="module" src="{{ 'component-inline-carousel.js' | asset_url }}"></script>
												{% endif -%}
											{%- endif %}
										</div>
									{% when 'pickup_availability' %}
										<div
											class="product-info__block"
											data-life-region
											id="{{ block.id }}"
											{{ block.shopify_attributes }}
										>
											{% assign pickup_popup_target = block.id | append: 'popup' %}
											{% assign pick_up_availabilities = section.settings.product.selected_or_first_available_variant.store_availabilities
												| where: 'pick_up_enabled', true
											%}
											{% if pick_up_availabilities.size > 0 -%}
												{%- assign closest_availability = pick_up_availabilities.first %}
												<div>
													{% if closest_availability.available %}
														<h2 class="product-info__heading">
															{{
																'products.product.pickup_availability.pick_up_available_at_html'
																| t: location_name: closest_availability.location.name
															}}
														</h2>
														<p class="text text--size--sm text--mute">{{ closest_availability.pick_up_time }}</p>
													{% else %}
														<h2 class="product-info__heading">
															{{
																'products.product.pickup_availability.pick_up_unavailable_at_html'
																| t: location_name: closest_availability.location.name
															}}
														</h2>
													{% endif %}
													{% if closest_availability.available %}
														<x-modal-trigger
															class="modal-trigger modal__link"
															target-id="{{ pickup_popup_target }}"
															tabindex="0"
															role="button"
														>
															{{ 'products.product.pickup_availability.view_store_info' | t }}
														</x-modal-trigger>
													{%- elsif closest_availability.available == false and pick_up_availabilities.size > 1 %}
														<x-modal-trigger
															class="modal-trigger modal__link"
															target-id="{{ pickup_popup_target }}"
															tabindex="0"
															role="button"
														>
															{{ 'products.product.pickup_availability.check_other_stores' | t }}
														</x-modal-trigger>
													{%- endif %}
												</div>
												<x-modal
													class="modal modal--animate--slide-right modal--full-height"
													id="{{ pickup_popup_target }}"
													style=""
												>
													<dialog class="modal__dialog" data-modal-element="dialog">
														<div class="modal__content" data-modal-element="content">
															<div class="product-info__pickup__modal">
																<div class="product-info__pickup__modal-header">
																	<div>
																		<h4 class="heading heading--size--h6">
																			{{ section.settings.product.selected_or_first_available_variant.product.title }}
																		</h4>
																		{% unless section.settings.product.selected_or_first_available_variant.product.has_only_default_variant %}
																			<p class="text text--size--xs text--mute">
																				{{ section.settings.product.selected_or_first_available_variant.title }}
																			</p>
																		{% endunless %}
																	</div>
																	<x-modal-close
																		class="modal-close product-info__pickup__modal-close modal-close--icon"
																		target-id="{{ pickup_popup_target }}"
																		tabindex="0"
																		role="button"
																		aria-label="{{ 'accessibility.close' | t }}"
																	>
																		<svg class="modal-close__icon">
																			<use  xlink:href="#svg-icon-x"></use>
																		</svg>
																	</x-modal-close>
																</div>
																<div
																	style="--stack-spacing:1.25"
																	class="stack product-info__pickup__modal-content stack--v"
																>
																	{% for pickup in pick_up_availabilities %}
																		<div>
																			{% assign pickup_address = pickup.location.address %}
																			<h2 class="product-info__heading">
																				{{ pickup.location.name | escape }}
																			</h2>
																			{% if pickup.available %}
																				<p class="text text--size--xs">
																					{{ 'products.product.pickup_availability.pick_up_available' | t }},
																					{{ pickup.pick_up_time | downcase }}
																				</p>
																			{% endif %}
																			<address class="product-info__pickup__address">
																				{{ pickup_address | format_address }}
																			</address>
																			{% if pickup_address.phone != blank -%}
																				<a class="product-info__pickup__phone" href="tel:{{ pickup_address.phone }}">
																					{{ pickup_address.phone }}
																				</a>
																			{%- endif %}
																		</div>
																	{% endfor %}
																</div>
															</div>
														</div>
													</dialog>
												</x-modal>
											{%- endif %}
										</div>
									{% when 'separator' %}
										<div class="product-info__block" {{ block.shopify_attributes }}>
											<div class="product-info__separator"></div>
										</div>
									{% when 'liquid_content' %}
										<div class="product-info__block" {{ block.shopify_attributes }}>
											{{ block.settings.liquid_content }}
										</div>
									{% when '@app' %}
										<div class="product-info__block" {{ block.shopify_attributes }}>
											{% render block %}
										</div>
								{% endcase -%}
							{%- endfor %}
						</div>
					</x-stage-loader>
					<script type="module" src="{{ 'component-stage-loader.js' | asset_url }}"></script>
				</x-product-info>

				<script type="module" src="{{ 'component-product-info.js' | asset_url }}"></script>
			</div>
		</div>
	</div>
</div>

<script type="application/ld+json">
	{{ section.settings.product | structured_data }}
</script>

{% schema %}
	{
		"name": "Featured product",
		"class": "featured-product-section",
		"presets": [
			{
				"name": "Featured product",
				"blocks": [
					{
						"type": "title"
					},
					{
						"type": "price"
					},
					{
						"type": "variant_picker"
					},
					{
						"type": "buy_buttons"
					}
				],
				"category": "Promotional"
			}
		],
		"disabled_on": {
			"templates": ["password"],
			"groups": ["header", "footer", "custom.overlay"]
		},
		"settings": [
			{
				"type": "product",
				"id": "product",
				"label": "Product"
			},
			{
				"type": "color_scheme",
				"id": "color_scheme",
				"label": "Color scheme",
				"default": "main"
			},
			{
				"type": "checkbox",
				"id": "enable_share",
				"label": "Show share icon",
				"default": true,
				"info": "Share functionality will not work in theme editor"
			},
			{
				"type": "header",
				"content": "Media"
			},
			{
				"type": "checkbox",
				"id": "sticky_media",
				"label": "Enable sticky content on desktop",
				"default": true
			},
			{
				"type": "select",
				"id": "media_position",
				"label": "Desktop position",
				"options": [
					{
						"value": "left",
						"label": "Left"
					},
					{
						"value": "right",
						"label": "Right"
					}
				],
				"default": "left"
			},
			{
				"type": "select",
				"id": "media_size",
				"label": "Desktop width",
				"options": [
					{
						"value": "small",
						"label": "Small"
					},
					{
						"value": "medium",
						"label": "Medium"
					},
					{
						"value": "large",
						"label": "Large"
					}
				],
				"default": "large"
			},
			{
				"type": "select",
				"id": "thumbs_position",
				"label": "Desktop thumbnails position",
				"options": [
					{
						"value": "aside",
						"label": "Side"
					},
					{
						"value": "bottom",
						"label": "Bottom"
					}
				],
				"default": "aside"
			},
			{
				"type": "select",
				"id": "gallery_ratio",
				"label": "Aspect ratio",
				"options": [
					{
						"value": "square",
						"label": "Square"
					},
					{
						"value": "portrait",
						"label": "Portrait"
					},
					{
						"value": "landscape",
						"label": "Landscape"
					}
				],
				"default": "square"
			},
			{
				"type": "select",
				"id": "media_fit",
				"label": "Media fit",
				"options": [
					{
						"value": "original",
						"label": "Original"
					},
					{
						"value": "fill",
						"label": "Fill"
					}
				],
				"default": "original"
			},
			{
				"type": "checkbox",
				"id": "enable_autoplay",
				"label": "Autoplay videos",
				"info": "Videos will be muted.",
				"default": false
			}
		],
		"blocks": [
			{
				"name": "Title",
				"type": "title",
				"limit": 1,
				"settings": [
					{
						"type": "select",
						"id": "title_size",
						"label": "Title size",
						"options": [
							{
								"value": "h3",
								"label": "Small"
							},
							{
								"value": "h2",
								"label": "Medium"
							},
							{
								"value": "h1",
								"label": "Large"
							}
						],
						"default": "h2"
					},
					{
						"type": "checkbox",
						"id": "show_vendor",
						"label": "Show vendor",
						"default": false
					},
					{
						"type": "checkbox",
						"id": "show_rating",
						"label": "Show rating",
						"default": true,
						"info": "To display a rating, add a product rating app. [Learn more](https://help.shopify.com/manual/online-store/themes/theme-structure/page-types/product-pages#product-rating-block)"
					}
				]
			},
			{
				"name": "Price",
				"type": "price",
				"limit": 1,
				"settings": [
					{
						"type": "checkbox",
						"id": "show_taxes_note",
						"label": "Show taxes note",
						"default": true
					}
				]
			},
			{
				"name": "Variant picker",
				"type": "variant_picker",
				"limit": 1,
				"settings": [
					{
						"type": "select",
						"id": "picker_type",
						"label": "Type",
						"options": [
							{
								"value": "dropdown",
								"label": "Dropdown"
							},
							{
								"value": "pills",
								"label": "Pills"
							}
						],
						"default": "pills"
					},
					{
						"type": "checkbox",
						"id": "enable_color_swatches",
						"label": "Enable color swatches",
						"default": true,
						"info": "Learn more about [swatches](https://help.shopify.com/en/manual/online-store/themes/theme-structure/theme-settings#options-with-swatches) on product options"
					},
					{
						"type": "text",
						"id": "variant_image_options",
						"label": "List of options with images",
						"info": "List product options that should always show as pills with variant image. Option titles must match exactly. Separate multiple options with commas (no spaces). Example: \"Color,Colour\""
					}
				]
			},
			{
				"name": "Buy buttons",
				"type": "buy_buttons",
				"limit": 1,
				"settings": [
					{
						"type": "checkbox",
						"id": "show_dynamic_checkout",
						"label": "Show dynamic checkout buttons",
						"default": true,
						"info": "Customers will see their preferred payment option. [Learn more](https://help.shopify.com/manual/using-themes/change-the-layout/dynamic-checkout)"
					},
					{
						"type": "checkbox",
						"id": "show_recipient_form",
						"label": "Gift card sending options",
						"default": true,
						"info": "Customers can add a personal message and schedule the send date. [Learn more](https://help.shopify.com/manual/online-store/themes/customizing-themes/add-gift-card-recipient-fields)"
					},
					{
						"type": "checkbox",
						"id": "show_payment_icons",
						"label": "Show payment icons",
						"default": true
					},
					{
						"type": "checkbox",
						"id": "show_qty_selector",
						"label": "Show quantity selector",
						"default": true
					}
				]
			},
			{
				"name": "Description",
				"type": "description",
				"limit": 1,
				"settings": [
					{
						"type": "checkbox",
						"id": "accordion",
						"label": "Show as collapsible tab",
