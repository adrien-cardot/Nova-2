<div class="section-wrapper section-wrapper--y-offset--padding color-scheme-{{ section.settings.color_scheme.id }}">
	<div class="container section-wrapper__body">
		<div
			class="aside-content aside-content--aside-position--{{ section.settings.media_position }} aside-content--aside-size--{{ section.settings.media_size }}"
			style=""
		>
			<aside class="aside-content__aside">
				{% if section.settings.product != blank -%}
					{{- 'component-product-gallery.css' | asset_url | stylesheet_tag }}
					{% if section.settings.product.selected_or_first_available_variant.featured_media != null -%}
						{%- assign current_media_index = section.settings.product.selected_or_first_available_variant.featured_media.position
							| minus: 1
						-%}
						{%- assign current_image_index = section.settings.product.selected_or_first_available_variant.featured_image.position
							| minus: 1
						-%}
					{%- else -%}
						{%- assign current_media_index = 0 -%}
						{%- assign current_image_index = 0 -%}
					{%- endif %}

					{% assign modal_id = section.id | append: 'gallery-modal' %}

					{% assign image_index = 0 %}

					<x-product-gallery
						class="product-gallery product-gallery--aspect-ratio--{{ section.settings.gallery_ratio }} {% if section.settings.media_fit == 'fill' %}product-gallery--fill-image{% endif %} product-gallery--layout--{{ section.settings.thumbs_position }} {% if section.settings.thumbs_position == 'aside' and section.settings.media_position == 'right' %}product-gallery--position-right{% endif %} {% if section.settings.product.media.size <= 1 %}product-gallery--without-thumbs{% endif %} {% if section.settings.sticky_media %}product-gallery--sticky{% endif %}"
						section-id="{{ section.id }}"
						style="--product-gallery-current-index: {{ current_media_index }};"
						modal-id="{{ modal_id }}"
						current-index="{{ current_media_index }}"
						current-image-index="{{ current_image_index }}"
						{% if section.settings.thumbs_position == 'aside' %}
							thumbs-aside
						{% endif %}
						{% if section.settings.enable_autoplay %}
							video-autoplay
						{% endif %}
					>
						<x-modal class="modal" id="{{ modal_id }}" style="">
							<dialog class="modal__dialog" data-modal-element="dialog">
								<div class="modal__content" data-modal-element="content">
									<x-modal-close
										class="modal-close product-gallery__modal-close"
										target-id="{{ modal_id }}"
										tabindex="0"
										role="button"
										aria-label="{{ 'accessibility.close' | t }}"
									>
										<svg>
											<use  xlink:href="#svg-icon-close"></use>
										</svg>
									</x-modal-close>
									<div class="product-gallery__modal" data-lenis-prevent>
										{% for image in section.settings.product.images %}
											<div class="product-gallery__modal-image-wrapper" data-product-gallery-element="modal-images">
												<picture class="product-gallery__modal-image">
													<source
														srcset="
															{% if image.width >= 375 %}{{ image | image_url: width: 375 }} 375w,{% endif %}
															{% if image.width >= 550 %}{{ image | image_url: width: 550 }} 550w,{% endif %}
															{% if image.width >= 750 %}{{ image | image_url: width: 750 }} 750w,{% endif %}
															{% if image.width >= 1100 %}{{ image | image_url: width: 1100 }} 1100w,{% endif %}
															{% if image.width >= 1500 %}{{ image | image_url: width: 1500 }} 1500w,{% endif %}
															{% if image.width >= 1780 %}{{ image | image_url: width: 1780 }} 1780w,{% endif %}
															{% if image.width >= 2000 %}{{ image | image_url: width: 2000 }} 2000w,{% endif %}
															{% if image.width >= 3000 %}{{ image | image_url: width: 3000 }} 3000w,{% endif %}
															{% if image.width >= 3840 %}{{ image | image_url: width: 3840 }} 3840w,{% endif %}
															{{ image | image_url }} {{ image.width }}w
														"
														sizes="100vw"
													>

													<img
														src="{{ image | image_url }}"
														alt="{{ image.alt | escape }}"
														width="{{ image.width }}"
														height="{{ image.height }}"
														loading="lazy"
														data-img-loading
														onload="this.removeAttribute(&#34;data-img-loading&#34;)"
													>
												</picture>
											</div>
										{% endfor %}
									</div>
								</div>
							</dialog>
						</x-modal>
						<div class="product-gallery__inner">
							{% if section.settings.product.media.size > 1 %}
								<div class="product-gallery__thumbs" data-lenis-prevent>
									{% for gallery_media in section.settings.product.media %}
										<div class="product-gallery__thumb">
											<input
												class="product-gallery__thumb-check"
												value="{{ forloop.index0 }}"
												type="radio"
												name="{{ section.id | append: 'product-gallery-thumb' }}"
												aria-label="{{ gallery_media.alt | escape }}"
												data-product-gallery-element="thumbs-check"
												{% if forloop.index0 == current_media_index %}
													checked
												{% endif %}
												{% if gallery_media.media_type == 'image' %}
													thumb-image-index="{{ image_index }}"
												{% endif %}
											>

											<picture class="product-gallery__thumb-image">
												<source srcset="{{ gallery_media.preview_image | image_url: width: 70 }} 1x, {{ gallery_media.preview_image | image_url: width: 140 }} 2x, {{ gallery_media.preview_image | image_url: width: 210 }} 3x">

												<img
													src="{{ gallery_media.preview_image | image_url }}"
													alt="{{ gallery_media.preview_image.alt | escape }}"
													width="{{ gallery_media.preview_image.width }}"
													height="{{ gallery_media.preview_image.height }}"
													loading="lazy"
													data-img-loading
													onload="this.removeAttribute(&#34;data-img-loading&#34;)"
												>
											</picture>
											<div class="product-gallery__thumb-dot"></div>
											{% unless gallery_media.media_type == 'image' %}
												<div class="product-gallery__thumb-icon-wrapper">
													{% if gallery_media.media_type == 'model' %}
														<svg>
															<use  xlink:href="#svg-icon-box-3d"></use>
														</svg>
													{% elsif gallery_media.media_type == 'video'
														or gallery_media.media_type == 'external_video'
													%}
														<svg>
															<use  xlink:href="#svg-icon-play"></use>
														</svg>
													{% endif %}
												</div>
											{% endunless %}
										</div>
										{% if gallery_media.media_type == 'image' -%}
											{%- assign image_index = image_index | plus: 1 -%}
										{%- endif -%}
									{%- endfor %}
								</div>
							{% endif %}
							<div class="product-gallery__container-wrapper" data-product-gallery-element="container-root">
								{% if section.settings.enable_share %}
									{% assign product_clean_url = section.settings.product.url | split: '?' | first -%}
									{%- assign url_to_share = shop.url | append: product_clean_url %}
									<div class="product-share">
										<x-share
											class="share"
											fallback-msg="{{ 'general.share.success_message' | t }}"
											url-to-share="{{ url_to_share }}"
											style="--share-icon-scale: 1.5;"
										>
											<svg class="share__icon">
												<use  xlink:href="#svg-icon-share"></use>
											</svg>
										</x-share>
										<script type="module" src="{{ 'component-share.js' | asset_url }}"></script>
									</div>
								{% endif %}
								<div class="product-gallery__container" data-product-gallery-element="track">
									{% for media in section.settings.product.media %}
										{% unless media.media_type == 'external_video' -%}
											{%- assign media_style_mod = media.media_type -%}
										{%- else -%}
											{%- assign media_style_mod = 'external-video' -%}
										{%- endunless %}

										{% if media.media_type == 'external_video' -%}
											{%- if section.settings.enable_autoplay -%}
												{%- if media.host == 'youtube' -%}
													{%- assign external_video_url = media | external_video_url: autoplay: '1', mute: '1' -%}
												{%- else -%}
													{%- assign external_video_url = media | external_video_url: autoplay: '1', muted: '1' -%}
												{%- endif -%}
											{%- else -%}
												{%- assign external_video_url = media | external_video_url -%}
											{%- endif -%}
										{%- endif %}

										<div
											class="product-gallery__media product-gallery__media--media-type--{{ media_style_mod }}"
											media-index="{{ media.position | minus: 1 }}"
											{% if media.media_type == 'video' or media.media_type == 'external_video' %}
												media-type-video
											{% endif %}
											{% if media.media_type == 'external_video' %}
												video-host="{{ media.host }}"
											{% endif %}
										>
											{% case media.media_type -%}
												{%- when 'image' %}
													<x-modal-trigger
														class="modal-trigger product-gallery__media__modal-trigger"
														target-id="{{ modal_id }}"
														aria-label="{{ media.alt }}"
													>
														<picture class="product-gallery__media__image">
															<source
																srcset="
																	{% if media.preview_image.width >= 375 %}{{ media.preview_image | image_url: width: 375 }} 375w,{% endif %}
																	{% if media.preview_image.width >= 550 %}{{ media.preview_image | image_url: width: 550 }} 550w,{% endif %}
																	{% if media.preview_image.width >= 750 %}{{ media.preview_image | image_url: width: 750 }} 750w,{% endif %}
																	{% if media.preview_image.width >= 1100 %}{{ media.preview_image | image_url: width: 1100 }} 1100w,{% endif %}
																	{% if media.preview_image.width >= 1500 %}{{ media.preview_image | image_url: width: 1500 }} 1500w,{% endif %}
																	{% if media.preview_image.width >= 1780 %}{{ media.preview_image | image_url: width: 1780 }} 1780w,{% endif %}
																	{% if media.preview_image.width >= 2000 %}{{ media.preview_image | image_url: width: 2000 }} 2000w,{% endif %}
																	{% if media.preview_image.width >= 3000 %}{{ media.preview_image | image_url: width: 3000 }} 3000w,{% endif %}
																	{{ media.preview_image | image_url }} {{ media.preview_image.width }}w
																"
																sizes="(min-width: 992px) 1066px, (max-width: 992px) 90vw"
															>

															<img
																src="{{ media.preview_image | image_url }}"
																alt="{{ media.preview_image.alt | escape }}"
																width="{{ media.preview_image.width }}"
																height="{{ media.preview_image.height }}"
																loading="lazy"
																data-img-loading
																onload="this.removeAttribute(&#34;data-img-loading&#34;)"
															>
														</picture>
													</x-modal-trigger>
												{%- when 'model' -%}
													{{-
														media
														| model_viewer_tag:
															toggleable: true,
															ar-modes: 'webxr scene-viewer quick-look',
															ar: 'true',
															camera-controls: 'true',
															tabindex: '-1'
													-}}
												{%- when 'video' -%}
													{{-
														media
														| video_tag:
															controls: true,
															autoplay: section.settings.enable_autoplay,
															muted: section.settings.enable_autoplay,
															tabindex: '-1'
													-}}
												{%- when 'external_video' -%}
													{{- external_video_url | external_video_tag: loading: 'lazy', tabindex: '-1' -}}
											{%- endcase %}
										</div>
									{% endfor %}
								</div>
							</div>
						</div>
					</x-product-gallery>
					<script type="module" src="{{ 'component-product-gallery.js' | asset_url }}"></script>
				{% else %}
					<div class="banner banner--placerholder-ar" style="--banner-placeholder-ar: 1;">
						{{
							0
							| modulo: 6
							| plus: 1
							| prepend: '-'
							| prepend: 'product'
							| placeholder_svg_tag: 'banner__placeholder'
						}}
					</div>
				{% endif %}
			</aside>
			<div class="aside-content__content">
				<x-product-info
					class="product-info"
					initial-option-ids="{{ section.settings.product.selected_or_first_available_variant.options | map: 'id' | join: ',' }}"
					product-url="{{ section.settings.product.url }}"
					section-id="{{ section.id }}"
				>
					{% assign product_form_id = section.id | append: '-product-form' %}
					<x-stage-loader class="stage-loader" section-id="{{ section.id }}">
						<div class="product-info__blocks">
							{% for block in section.blocks -%}
								{%- case block.type -%}
									{%- when 'title' %}
										<div class="product-info__block" {{ block.shopify_attributes }}>
											<div class="product-info__header">
												{% if block.settings.show_vendor %}
													<div class="product-info__header__meta">
														{% if block.settings.show_vendor -%}
															<span>{{ section.settings.product.vendor }}</span>
														{%- endif %}
													</div>
												{% endif %}
												{% if block.settings.show_rating -%}
													{%- if section.settings.product.metafields.reviews.rating.value.rating != blank %}
														<div class="star-rating product-info__header__rating {% if settings.colors_yellow_starrating %}star-rating--colored{% endif %}">
															<div class="star-rating__stars">
																{% assign product_rating = section.settings.product.metafields.reviews.rating.value.rating -%}
																{%- for i in (0..4) %}
																	<div class="star-rating__icon" style="--star-rating-offset: {{ product_rating }};">
																		<span class="star-rating__icon-front"></span>
																		<span class="star-rating__icon-back"></span>
																	</div>
																	{% assign product_rating = product_rating | minus: 1 -%}
																{%- endfor %}
															</div>

															{% if section.settings.product.metafields.reviews.rating_count != blank %}
																<div class="star-rating__counter">
																	{{
																		'accessibility.reviews_count_html'
																		| t: count: section.settings.product.metafields.reviews.rating_count
																	}}
																</div>
															{% endif %}
														</div>
													{% endif -%}
												{%- endif %}
												<h1 class="heading heading--size--{{ block.settings.title_size }} product-info__header__heading">
													{% if section.settings.product != null -%}
														{{- section.settings.product.title | escape -}}
													{%- else -%}
														{{- 'onboarding.product_title' | t -}}
													{%- endif %}
												</h1>
											</div>
										</div>
									{% when 'price' %}
										<div
											class="product-info__block"
											data-life-region
											id="{{ block.id }}"
											{{ block.shopify_attributes }}
										>
											{% if section.settings.product != blank -%}
												{%- if section.settings.product.selected_or_first_available_variant != null %}
													<div class="product-info__price">
														{% render 'price',
															src: section.settings.product.selected_or_first_available_variant,
															size: 'xl'
														%}
														{% if block.settings.show_taxes_note -%}
															{%- if cart.taxes_included
																or cart.duties_included
																or shop.shipping_policy.body != blank
															%}
																<div class="product-tax-note product-info__price__tax">
																	{% if cart.duties_included and cart.taxes_included %}
																		<p class="text text--size--sm product-tax-note__line text--mute">
																			{{ 'products.product.duties_and_taxes_included' | t }}
																		</p>
																	{% elsif cart.taxes_included %}
																		<p class="text text--size--sm product-tax-note__line text--mute">
																			{{ 'products.product.taxes_included' | t }}
																		</p>
																	{% elsif cart.duties_included %}
																		<p class="text text--size--sm product-tax-note__line text--mute">
																			{{ 'products.product.duties_included' | t }}
																		</p>
																	{% endif -%}
																	{%- if shop.shipping_policy.body != blank %}
																		<p class="text text--size--sm product-tax-note__line text--mute">
																			{{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
																		</p>
																	{% endif %}
																</div>
															{% endif -%}
														{%- endif %}
														{% assign product_installment_form_id = section.id | append: 'product-form-installment' %}
														{% form 'product', section.settings.product, id: product_installment_form_id %}
															<input
																type="hidden"
																name="id"
																value="{{ section.settings.product.selected_or_first_available_variant.id }}"
															>
															{{ form | payment_terms }}
														{% endform %}
													</div>
												{% endif -%}
											{%- else -%}
												{%- render 'price', size: 'lg' -%}
											{%- endif %}
										</div>
									{% when 'variant_picker' -%}
										{%- unless section.settings.product.has_only_default_variant %}
											<div
												class="product-info__block"
												data-life-region
												id="{{ block.id }}"
												{{ block.shopify_attributes }}
											>
												{% assign image_options = block.settings.variant_image_options | split: ',' %}
												<x-product-variant-picker
													class="product-variant-picker"
													product-id="{{ section.settings.product.id }}"
													section-id="{{ section.id }}"
												>
													<div style="--stack-spacing:0.75" class="stack stack--v">
														{% for option in section.settings.product.options_with_values -%}
															{%- if image_options contains option.name -%}
																{%- assign use_variant_image = true -%}
															{%- else -%}
																{%- assign use_variant_image = false -%}
															{%- endif -%}
															{%- assign use_dropdown = false -%}
															{%- if block.settings.picker_type == 'dropdown' and use_variant_image == false -%}
																{%- assign use_dropdown = true -%}
															{%- endif %}
															<div class="product-variant-picker__option">
																<div class="product-variant-picker__option-heading">
																	<span class="product-variant-picker__option-heading-name"> {{ option.name }}: </span>
																	{% unless use_dropdown -%}
																		<span>{{ option.selected_value }}</span>
																	{%- endunless %}
																</div>
																{% if use_dropdown -%}
																	<x-drop-down class="drop-down product-variant-picker__drop drop-down--full">
																		<details class="drop-down__details" data-drop-down-element="details">
																			<summary class="drop-down__header product-variant-picker__drop-header">
																				<span class="product-variant-picker__drop-swatch-header">
																					{% if option.selected_value.swatch
																						and block.settings.enable_color_swatches
																					-%}
																						<span class="product-variant-picker__swatch product-variant-picker__option-drop-swatch">
																							<span
																								class="product-variant-picker__swatch__bg"
																								style="background: {% if option.selected_value.swatch.image %}url({{ option.selected_value.swatch.image | image_url: width: 100 }}){% else %}{{ option.selected_value.swatch.color }}{% endif %};"
																							></span>
																						</span>
																					{%- endif %}
																					{{ option.selected_value.name }}
																				</span>
																				<svg class="drop-down__header-icon">
																					<use  xlink:href="#svg-icon-arrow-down"></use>
																				</svg>
																			</summary>
																			<div class="drop-down__drop" data-lenis-prevent>
																				{% for option_value in option.values -%}
																					<label class="product-variant-picker__option-drop-value {% unless option_value.variant.available %}product-variant-picker__option-drop-value--none{% endunless %}">
																						<input
																							type="radio"
																							class="visually-hidden"
																							value="{{ option_value.id }}"
																							form
																							name="{{ section.id | append: 'select-radio' | append: option.name }}"
																							data-media-position="{{ option_value.variant.featured_media.position }}"
																							data-position="{{ option.position }}"
																							{% if option_value.selected %}
																								checked
																							{% endif %}
																						>
																						{% if option_value.swatch and block.settings.enable_color_swatches -%}
																							<span class="product-variant-picker__swatch product-variant-picker__option-drop-swatch">
																								<span
																									class="product-variant-picker__swatch__bg"
																									style="background: {% if option_value.swatch.image %}url({{ option_value.swatch.image | image_url: width: 100 }}){% else %}{{ option_value.swatch.color }}{% endif %};"
																								></span>
																							</span>
																						{%- endif %}
																						{{ option_value.name }}
																						{% if option_value.product_url != blank -%}
																							<a
																								class="stretched-link"
																								href="{{ option_value.product_url | append: '?variant=' | append: option_value.variant.id }}"
																							>
																							</a>
																						{%- endif %}
																					</label>
																				{%- endfor %}
																			</div>
																		</details>
																	</x-drop-down>
																	<script type="module" src="{{ 'component-drop-down.js' | asset_url }}"></script>
																{% else %}
																	<div style="--stack-spacing:0.75" class="stack stack--wrap">
																		{% for option_value in option.values -%}
																			<label class="product-variant-picker__option-value">
																				<input
																					type="radio"
																					class="visually-hidden"
																					value="{{ option_value.id }}"
																					form
																					name="{{ section.id | append: 'select-radio' | append: option.name }}"
																					data-media-position="{{ option_value.variant.featured_media.position }}"
																					data-position="{{ option.position }}"
																					{% if option_value.selected %}
																						checked
																					{% endif %}
																				>

																				{% if use_variant_image %}
																					{% if option_value.variant -%}
																						{%- assign variant_image = option_value.variant.image -%}
																					{%- else -%}
																						{%- for variant in section.settings.product.variants limit: 50 -%}
																							{%- if variant.options contains option_value.name -%}
																								{%- assign variant_image = variant.image -%}
																								{%- break -%}
																							{%- endif -%}
																						{%- endfor -%}
																					{%- endif %}

																					<span class="product-variant-picker__image product-variant-picker__option-value-pill">
																						{% if variant_image %}
																							<picture class="product-variant-picker__image__image">
																								<source srcset="{{ variant_image | image_url: width: 80 }} 1x, {{ variant_image | image_url: width: 160 }} 2x, {{ variant_image | image_url: width: 240 }} 3x">

																								<img
																									src="{{ variant_image | image_url }}"
																									alt="{{ variant_image.alt | escape }}"
																									width="{{ variant_image.width }}"
																									height="{{ variant_image.height }}"
																									loading="lazy"
																									data-img-loading
																									onload="this.removeAttribute(&#34;data-img-loading&#34;)"
																								>
																							</picture>
																						{%- endif %}
																					</span>
																				{%- elsif option_value.swatch and block.settings.enable_color_swatches -%}
